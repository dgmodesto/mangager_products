    {
      "Key": "registerproduct",
      "Config": [
        {
          "Name": "Register Product",
          "Version": "v1",
          "Url": "https://localhost:5001/swagger/v1/swagger.json"

        }
      ]
    }
  ],

  "GlobalConfiguration": {}
}